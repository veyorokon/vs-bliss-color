{
  "name": "Bliss",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": "#8fa1b3",
    "variable.constant": "#e59758",
    "variable.defaultLibrary": "#c0c5ce"
